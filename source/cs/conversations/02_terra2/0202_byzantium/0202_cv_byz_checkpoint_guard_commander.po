msgctxt "conversations/02_terra2/0202_byzantium/0202_cv_byz_checkpoint_guard_commander:93:DefaultText"
msgid "<moving/travel music playing>"
msgstr ""

#: conversations/02_terra2/0202_byzantium/0202_cv_byz_checkpoint_guard_commander:94
msgctxt "conversations/02_terra2/0202_byzantium/0202_cv_byz_checkpoint_guard_commander:94:DefaultText"
msgid "I've got a Hammersmith Grenade Launcher. It's yours if you let me through."
msgstr ""

