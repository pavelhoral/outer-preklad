#: conversations/04_monarch/0420/0403_cv_august_van_noy:1
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:1:DefaultText"
msgid "Spreading the good word, one bullet at a time."
msgstr "Šíření dobrého slova, jedna kulka za druhou."

#: conversations/04_monarch/0420/0403_cv_august_van_noy:2
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:2:DefaultText"
msgid "We ... we did it. Guess I was wrong."
msgstr "My... my jsme to dokázali. Asi jsem se mýlil."

#: conversations/04_monarch/0420/0403_cv_august_van_noy:3
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:3:DefaultText"
msgid "Talk to May."
msgstr "Promluvte si s Mayem."

#: conversations/04_monarch/0420/0403_cv_august_van_noy:28
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:28:DefaultText"
msgid "May, you idiot ... I told you this wasn't worth it ..."
msgstr "Mayi, ty idiote... Říkal jsem ti, že to za to nestojí..."

#: conversations/04_monarch/0420/0403_cv_august_van_noy:31
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:31:DefaultText"
msgid "Hey, thanks for the help."
msgstr "Ahoj, díky za pomoc."

#: conversations/04_monarch/0420/0403_cv_august_van_noy:32
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:32:DefaultText"
msgid "We ain't going back."
msgstr "Už se nevrátíme."

#: conversations/04_monarch/0420/0403_cv_august_van_noy:33
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:33:DefaultText"
msgid "Ready to kick some ass?"
msgstr "Jste připraveni nakopat někomu zadek?"

#: conversations/04_monarch/0420/0403_cv_august_van_noy:34
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:34:DefaultText"
msgid "Let's do this!"
msgstr "Jdeme na to!"

#: conversations/04_monarch/0420/0403_cv_august_van_noy:37
msgctxt "conversations/04_monarch/0420/0403_cv_august_van_noy:37:DefaultText"
msgid "Hey, Cap!"
msgstr "Hej, Cape!"

