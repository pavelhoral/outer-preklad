#: conversations/04_monarch/0402/0402_cv_nyoka_crime:1
msgctxt "conversations/04_monarch/0402/0402_cv_nyoka_crime:1:DefaultText"
msgid "Hope you know what you're doing."
msgstr "Doufám, že víte, co děláte."

#: conversations/04_monarch/0402/0402_cv_nyoka_crime:2
msgctxt "conversations/04_monarch/0402/0402_cv_nyoka_crime:2:DefaultText"
msgid "I'll pay you <oeiglobal>Bribe_Amount</oeiglobal> bits to look the other way."
msgstr "Zaplatím vám <oeiglobal>Částka úplatku</oeiglobal>, abyste se dívali jinam."

#: conversations/04_monarch/0402/0402_cv_nyoka_crime:3
msgctxt "conversations/04_monarch/0402/0402_cv_nyoka_crime:3:DefaultText"
msgid "Oh, I wasn't doing anything. Honest."
msgstr "Nic jsem nedělal. Upřímně."

#: conversations/04_monarch/0402/0402_cv_nyoka_crime:4
msgctxt "conversations/04_monarch/0402/0402_cv_nyoka_crime:4:DefaultText"
msgid "Mind your own business."
msgstr "Hleďte si svého."

#: conversations/04_monarch/0402/0402_cv_nyoka_crime:5
msgctxt "conversations/04_monarch/0402/0402_cv_nyoka_crime:5:DefaultText"
msgid "I was just leaving."
msgstr "Právě jsem odcházel."

#: conversations/04_monarch/0402/0402_cv_nyoka_crime:6
msgctxt "conversations/04_monarch/0402/0402_cv_nyoka_crime:6:DefaultText"
msgid "<Attack>"
msgstr "<Útok>"