#: conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:1
msgctxt "conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:1:DefaultText"
msgid "This way! We need to push through and take that bridge!"
msgstr "Tudy! Musíme prorazit a dobýt ten most!"

#: conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:2
msgctxt "conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:2:DefaultText"
msgid "We got 'em all."
msgstr "Máme je všechny."

#: conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:3
msgctxt "conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:3:DefaultText"
msgid "We'd be dead if it wasn't for you, Captain."
msgstr "Nebýt vás, byli bychom mrtví, kapitáne."

#: conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:4
msgctxt "conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:4:DefaultText"
msgid "We've gotta keep them out of Stellar Bay."
msgstr "Musíme je udržet mimo Hvězdnou zátoku."

#: conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:5
msgctxt "conversations/04_monarch/0401/0401_cv_msi_soldier_barks_f:5:DefaultText"
msgid "Point and shoot, just point and shoot..."
msgstr "Zaměř a foť, jen zaměř a foť..."