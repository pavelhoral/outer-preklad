msgctxt "conversations/companions/g_companion_parvati_bar_crawl_new:408:DefaultText"
msgid "Shush, you."
msgstr ""

#: conversations/companions/g_companion_parvati_bar_crawl_new:409
msgctxt "conversations/companions/g_companion_parvati_bar_crawl_new:409:DefaultText"
msgid "Here we go..."
msgstr ""

#: conversations/companions/g_companion_parvati_bar_crawl_new:410
msgctxt "conversations/companions/g_companion_parvati_bar_crawl_new:410:DefaultText"
msgid "Down it goes!"
msgstr ""

#: conversations/companions/g_companion_parvati_bar_crawl_new:411
msgctxt "conversations/companions/g_companion_parvati_bar_crawl_new:411:DefaultText"
msgid "Down it goes!"
msgstr ""

#: conversations/companions/g_companion_parvati_bar_crawl_new:413
msgctxt "conversations/companions/g_companion_parvati_bar_crawl_new:413:DefaultText"
msgid "Hydration, here I come."
msgstr ""

#: conversations/companions/g_companion_parvati_bar_crawl_new:414
msgctxt "conversations/companions/g_companion_parvati_bar_crawl_new:414:DefaultText"
msgid "Not to worry. We'll always be here for you, Ms. Holcomb."
msgstr ""