#: chatter/parent_companionparvati:386
msgctxt "chatter/parent_companionparvati:386:DefaultText"
msgid "Real smooth."
msgstr "Skutečně hladké."

#: chatter/parent_companionparvati:387
msgctxt "chatter/parent_companionparvati:387:DefaultText"
msgid "Real slick."
msgstr "Opravdu svižné."

#: chatter/parent_companionparvati:388
msgctxt "chatter/parent_companionparvati:388:DefaultText"
msgid "Oh! You got 'em!"
msgstr "Máš je!"

#: chatter/parent_companionparvati:390
msgctxt "chatter/parent_companionparvati:390:DefaultText"
msgid "What were we talkin' about?"
msgstr "O čem jsme to mluvili?"

#: chatter/parent_companionparvati:391
msgctxt "chatter/parent_companionparvati:391:DefaultText"
msgid "So, anyway..."
msgstr "Takže..."

#: chatter/parent_companionparvati:392
msgctxt "chatter/parent_companionparvati:392:DefaultText"
msgid "Not a peep from me."
msgstr "Já ani nepípnu."

#: chatter/parent_companionparvati:393
msgctxt "chatter/parent_companionparvati:393:DefaultText"
msgid "Got it. Inside voices."
msgstr "Mám to. Vnitřní hlasy."

#: chatter/parent_companionparvati:394
msgctxt "chatter/parent_companionparvati:394:DefaultText"
msgid "Shh. They're close."
msgstr "Pšššt. Jsou blízko."

#: chatter/parent_companionparvati:396
msgctxt "chatter/parent_companionparvati:396:DefaultText"
msgid "Let's talk when you're free."
msgstr "Promluvíme si, až budeš mít čas."

#: chatter/parent_companionparvati:397
msgctxt "chatter/parent_companionparvati:397:DefaultText"
msgid "Ellie's down!"
msgstr "Ellie je na dně!"

#: chatter/parent_companionparvati:398
msgctxt "chatter/parent_companionparvati:398:DefaultText"
msgid "Cap! Ellie's down!"
msgstr "Čepice! Ellie je na zemi!"

#: chatter/parent_companionparvati:399
msgctxt "chatter/parent_companionparvati:399:DefaultText"
msgid "Ellie needs help!"
msgstr "Ellie potřebuje pomoc!"

#: chatter/parent_companionparvati:400
msgctxt "chatter/parent_companionparvati:400:DefaultText"
msgid "SAM's down!"
msgstr "SAM je mrtvý!"

#: chatter/parent_companionparvati:401
msgctxt "chatter/parent_companionparvati:401:DefaultText"
msgid "Get up, SAM!"
msgstr "Vstávej, SAMe!"

#: chatter/parent_companionparvati:403
msgctxt "chatter/parent_companionparvati:403:DefaultText"
msgid "Max! Hold on!"
msgstr "Maxi! Vydržte!"

#: chatter/parent_companionparvati:404
msgctxt "chatter/parent_companionparvati:404:DefaultText"
msgid "Max, no!"
msgstr "Maxi, ne!"

#: chatter/parent_companionparvati:406
msgctxt "chatter/parent_companionparvati:406:DefaultText"
msgid "Felix is hurt!"
msgstr "Felix je zraněný!"

#: chatter/parent_companionparvati:407
msgctxt "chatter/parent_companionparvati:407:DefaultText"
msgid "Felix is down!"
msgstr "Felix je mrtvý!"

#: chatter/parent_companionparvati:408
msgctxt "chatter/parent_companionparvati:408:DefaultText"
msgid "Cap! We gotta help Felix!"
msgstr "Čepice! Musíme pomoci Felixovi!"

#: chatter/parent_companionparvati:409
msgctxt "chatter/parent_companionparvati:409:DefaultText"
msgid "Nyoka's been hit!"
msgstr "Nyoka byla zasažena!"

#: chatter/parent_companionparvati:410
msgctxt "chatter/parent_companionparvati:410:DefaultText"
msgid "Captain! Nyoka needs help!"
msgstr "Kapitáne! Nyoka potřebuje pomoc!"

#: chatter/parent_companionparvati:411
msgctxt "chatter/parent_companionparvati:411:DefaultText"
msgid "Nyoka! Hold on!"
msgstr "Nyoka! Vydržte!"

#: chatter/parent_companionparvati:412
msgctxt "chatter/parent_companionparvati:412:DefaultText"
msgid "Hi, Cap."
msgstr "Ahoj, Cap."

#: chatter/parent_companionparvati:413
msgctxt "chatter/parent_companionparvati:413:DefaultText"
msgid "Ain't that keen!"
msgstr "Není to tak nadšené?"

#: chatter/parent_companionparvati:415
msgctxt "chatter/parent_companionparvati:415:DefaultText"
msgid "You guys all right?"
msgstr "Jste v pořádku?"

#: chatter/parent_companionparvati:416
msgctxt "chatter/parent_companionparvati:416:DefaultText"
msgid "Everybody okay? Great!"
msgstr "Všichni v pořádku? Skvěle!"

#: chatter/parent_companionparvati:417
msgctxt "chatter/parent_companionparvati:417:DefaultText"
msgid "Everybody upright?"
msgstr "Všichni vzpřímeně?"

#: chatter/parent_companionparvati:418
msgctxt "chatter/parent_companionparvati:418:DefaultText"
msgid "Look out!"
msgstr "Pozor!"

#: chatter/parent_companionparvati:419
msgctxt "chatter/parent_companionparvati:419:DefaultText"
msgid "All together!"
msgstr "Všichni dohromady!"

#: chatter/parent_companionparvati:420
msgctxt "chatter/parent_companionparvati:420:DefaultText"
msgid "I got your back, Captain!"
msgstr "Kryju ti záda, kapitáne!"

#: chatter/parent_companionparvati:421
msgctxt "chatter/parent_companionparvati:421:DefaultText"
msgid "Got it. Heads down."
msgstr "Mám to. Hlavu dolů."

#: chatter/parent_companionparvati:423
msgctxt "chatter/parent_companionparvati:423:DefaultText"
msgid "Hey, good work."
msgstr "Dobrá práce."

#: chatter/parent_companionparvati:424
msgctxt "chatter/parent_companionparvati:424:DefaultText"
msgid "Hey, good work."
msgstr "Dobrá práce."

#: chatter/parent_companionparvati:426
msgctxt "chatter/parent_companionparvati:426:DefaultText"
msgid "Hey, good work."
msgstr "Dobrá práce."

#: chatter/parent_companionparvati:427
msgctxt "chatter/parent_companionparvati:427:DefaultText"
msgid "Hey, good work."
msgstr "Dobrá práce."

#: chatter/parent_companionparvati:428
msgctxt "chatter/parent_companionparvati:428:DefaultText"
msgid "Hey, good work."
msgstr "Dobrá práce."

#: chatter/parent_companionparvati:429
msgctxt "chatter/parent_companionparvati:429:DefaultText"
msgid "Hey, good work."
msgstr "Dobrá práce."

#: chatter/parent_companionparvati:430
msgctxt "chatter/parent_companionparvati:430:DefaultText"
msgid "Hey, good work."
msgstr "Dobrá práce."

#: chatter/parent_companionparvati:431
msgctxt "chatter/parent_companionparvati:431:DefaultText"
msgid "Hey, good work."
msgstr "Dobrá práce."

#: chatter/parent_companionparvati:432
msgctxt "chatter/parent_companionparvati:432:DefaultText"
msgid "Not even a scratch."
msgstr "Ani škrábnutí."

#: chatter/parent_companionparvati:433
msgctxt "chatter/parent_companionparvati:433:DefaultText"
msgid "Finally."
msgstr "Nakonec."

#: chatter/parent_companionparvati:434
msgctxt "chatter/parent_companionparvati:434:DefaultText"
msgid "We need to talk."
msgstr "Musíme si promluvit."

#: chatter/parent_companionparvati:435
msgctxt "chatter/parent_companionparvati:435:DefaultText"
msgid "Well hey there."
msgstr "Ahoj."

#: chatter/parent_companionparvati:436
msgctxt "chatter/parent_companionparvati:436:DefaultText"
msgid "I'll fix you right up!"
msgstr "Hned ti to zařídím!"

#: chatter/parent_companionparvati:437
msgctxt "chatter/parent_companionparvati:437:DefaultText"
msgid "I see a nail!"
msgstr "Vidím hřebík!"

#: chatter/parent_companionparvati:438
msgctxt "chatter/parent_companionparvati:438:DefaultText"
msgid "You got it, Captain!"
msgstr "Máš to mít, kapitáne!"

#: chatter/parent_companionparvati:439
msgctxt "chatter/parent_companionparvati:439:DefaultText"
msgid "Hee-ya!!"
msgstr "Hee-ya!!"

#: chatter/parent_companionparvati:446
msgctxt "chatter/parent_companionparvati:446:DefaultText"
msgid "Sorry!"
msgstr "Omlouváme se!"

#: chatter/parent_companionparvati:447
msgctxt "chatter/parent_companionparvati:447:DefaultText"
msgid "It's nothing personal, I swear!"
msgstr "Není to nic osobního, přísahám!"

#: chatter/parent_companionparvati:448
msgctxt "chatter/parent_companionparvati:448:DefaultText"
msgid "No hard feelings, okay?"
msgstr "Nic ve zlém, ano?"

#: chatter/parent_companionparvati:456
msgctxt "chatter/parent_companionparvati:456:DefaultText"
msgid "Here I go!"
msgstr "Tady to mám!"

#: chatter/parent_companionparvati:457
msgctxt "chatter/parent_companionparvati:457:DefaultText"
msgid "Here goes nothing!"
msgstr "Tady se nic neděje!"

#: chatter/parent_companionparvati:459
msgctxt "chatter/parent_companionparvati:459:DefaultText"
msgid "Cap', watch out!"
msgstr "Pozor, kapitáne!"

#: chatter/parent_companionparvati:460
msgctxt "chatter/parent_companionparvati:460:DefaultText"
msgid "Cap', behind you!"
msgstr "Kapitáne, za tebou!"

#: chatter/parent_companionparvati:461
msgctxt "chatter/parent_companionparvati:461:DefaultText"
msgid "Behind you, Captain!"
msgstr "Za vámi, kapitáne!"

#: chatter/parent_companionparvati:462
msgctxt "chatter/parent_companionparvati:462:DefaultText"
msgid "Watch out, Cap'!"
msgstr "Pozor, kapitáne!"

#: chatter/parent_companionparvati:464
msgctxt "chatter/parent_companionparvati:464:DefaultText"
msgid "Cap', to your left!"
msgstr "Cap', vlevo!"

#: chatter/parent_companionparvati:465
msgctxt "chatter/parent_companionparvati:465:DefaultText"
msgid "On your left, Captain!"
msgstr "Po vaší levici, kapitáne!"

#: chatter/parent_companionparvati:467
msgctxt "chatter/parent_companionparvati:467:DefaultText"
msgid "Cap', to your right!"
msgstr "Kapitáne, vpravo!"

#: chatter/parent_companionparvati:468
msgctxt "chatter/parent_companionparvati:468:DefaultText"
msgid "On your right Cap'!"
msgstr "Vpravo, kapitáne!"

#: chatter/parent_companionparvati:488
msgctxt "chatter/parent_companionparvati:488:DefaultText"
msgid "What do you want?"
msgstr "Co chcete?"