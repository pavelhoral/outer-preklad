msgstr "<Bolestivé chrčení>"

#: chatter/parents/chp_outlaw:234
msgctxt "chatter/parents/chp_outlaw:234:DefaultText"
msgid "Whatever."
msgstr "To je jedno."

#: chatter/parents/chp_outlaw:235
msgctxt "chatter/parents/chp_outlaw:235:DefaultText"
msgid "Save me a piece!"
msgstr "Schovejte mi kousek!"

#: chatter/parents/chp_outlaw:237
msgctxt "chatter/parents/chp_outlaw:237:DefaultText"
msgid "Here we go!"
msgstr "A je to tady!"

#: chatter/parents/chp_outlaw:239
msgctxt "chatter/parents/chp_outlaw:239:DefaultText"
msgid "I ain't dying today!"
msgstr "Dneska neumřu!"

#: chatter/parents/chp_outlaw:240
msgctxt "chatter/parents/chp_outlaw:240:DefaultText"
msgid "Motherfucker!"
msgstr "Zkurvysyn!"

#: chatter/parents/chp_outlaw:241
msgctxt "chatter/parents/chp_outlaw:241:DefaultText"
msgid "Huh?"
msgstr "Cože?"

#: chatter/parents/chp_outlaw:242
msgctxt "chatter/parents/chp_outlaw:242:DefaultText"
msgid "You ain't fooling me."
msgstr "Mě neoklameš."

#: chatter/parents/chp_outlaw:243
msgctxt "chatter/parents/chp_outlaw:243:DefaultText"
msgid "Die!"
msgstr "Zemři!"

#: chatter/parents/chp_outlaw:244
msgctxt "chatter/parents/chp_outlaw:244:DefaultText"
msgid "Tallyho, motherfucker!"
msgstr "Tallyho, hajzle!"

#: chatter/parents/chp_outlaw:245
msgctxt "chatter/parents/chp_outlaw:245:DefaultText"
msgid "Damn it!"
msgstr "Sakra!"

#: chatter/parents/chp_outlaw:246
msgctxt "chatter/parents/chp_outlaw:246:DefaultText"
msgid "Shit!"
msgstr "Do prdele!"

#: chatter/parents/chp_outlaw:247
msgctxt "chatter/parents/chp_outlaw:247:DefaultText"
msgid "Fuck!"
msgstr "Kurva!"

#: chatter/parents/chp_outlaw:248
msgctxt "chatter/parents/chp_outlaw:248:DefaultText"
msgid "We gonna parley?"
msgstr "Budeme vyjednávat?"

#: chatter/parents/chp_outlaw:249
msgctxt "chatter/parents/chp_outlaw:249:DefaultText"
msgid "How stupid do you think I am?"
msgstr "Jak hloupý si myslíš, že jsem?"