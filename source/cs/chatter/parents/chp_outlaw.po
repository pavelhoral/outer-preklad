msgctxt "chatter/parents/chp_outlaw:207:DefaultText"
msgid "Can't see!"
msgstr "Nevidím!"

#: chatter/parents/chp_outlaw:208
msgctxt "chatter/parents/chp_outlaw:208:DefaultText"
msgid "My eyes!"
msgstr "Moje oči!"

#: chatter/parents/chp_outlaw:209
msgctxt "chatter/parents/chp_outlaw:209:DefaultText"
msgid "I'm blind!"
msgstr "Jsem slepý!"

#: chatter/parents/chp_outlaw:213
msgctxt "chatter/parents/chp_outlaw:213:DefaultText"
