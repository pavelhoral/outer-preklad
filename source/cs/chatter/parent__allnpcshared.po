#: chatter/parent__allnpcshared:287
msgctxt "chatter/parent__allnpcshared:287:DefaultText"
msgid "Wrong move!"
msgstr "Špatný krok!"

#: chatter/parent__allnpcshared:291
msgctxt "chatter/parent__allnpcshared:291:DefaultText"
msgid "You and I never saw each other."
msgstr "My dva jsme se nikdy neviděli."

#: chatter/parent__allnpcshared:294
msgctxt "chatter/parent__allnpcshared:294:DefaultText"
msgid "You can't hide forever!"
msgstr "Nemůžeš se schovávat věčně!"

#: chatter/parent__allnpcshared:295
msgctxt "chatter/parent__allnpcshared:295:DefaultText"
msgid "You can't hide from me!"
msgstr "Přede mnou se neschováš!"

#: chatter/parent__allnpcshared:296
msgctxt "chatter/parent__allnpcshared:296:DefaultText"
msgid "You can't hide from the Law!"
msgstr "Před zákonem se neschováš!"

#: chatter/parent__allnpcshared:299
msgctxt "chatter/parent__allnpcshared:299:DefaultText"
msgid "You know, you made a real generous argument."
msgstr "Víš, předložil jsi opravdu velkorysý argument."

#: chatter/parent__allnpcshared:302
msgctxt "chatter/parent__allnpcshared:302:DefaultText"
msgid "You picked the wrong damn day!"
msgstr "Vybral sis špatný den!"

#: chatter/parent__allnpcshared:306
msgctxt "chatter/parent__allnpcshared:306:DefaultText"
msgid "You take me for a fool?!"
msgstr "Máš mě za blázna?!"

#: chatter/parent__allnpcshared:308
msgctxt "chatter/parent__allnpcshared:308:DefaultText"
msgid "You think I'm an idiot?!"
msgstr "Myslíte si, že jsem idiot?!"

#: chatter/parent__allnpcshared:309
msgctxt "chatter/parent__allnpcshared:309:DefaultText"
msgid "You! Drop that magpick!"
msgstr "Ty! Zahoď ten magpick!"

#: chatter/parent__allnpcshared:311
msgctxt "chatter/parent__allnpcshared:311:DefaultText"
msgid "You're dead!"
msgstr "Jsi mrtvý!"

#: chatter/parent__allnpcshared:313
msgctxt "chatter/parent__allnpcshared:313:DefaultText"
msgid "You're not fooling me."
msgstr "Mě neoklamete."

#: chatter/parent__allnpcshared:316
msgctxt "chatter/parent__allnpcshared:316:DefaultText"
msgid "Carry on."
msgstr "Pokračujte."

#: chatter/parent__allnpcshared:317
msgctxt "chatter/parent__allnpcshared:317:DefaultText"
msgid "I don't have time for this."
msgstr "Nemám na to čas."

#: chatter/parent__allnpcshared:318
msgctxt "chatter/parent__allnpcshared:318:DefaultText"
msgid "Got my sights on you."
msgstr "Mám na tebe zálusk."

#: chatter/parent__allnpcshared:319
msgctxt "chatter/parent__allnpcshared:319:DefaultText"
msgid "As you were."
msgstr "Jak jsi to dělal ty."

#: chatter/parent__allnpcshared:320
msgctxt "chatter/parent__allnpcshared:320:DefaultText"
msgid "Move along."
msgstr "Pokračujte."

#: chatter/parent__allnpcshared:321
msgctxt "chatter/parent__allnpcshared:321:DefaultText"
msgid "Staying long?"
msgstr "Zůstáváte dlouho?"

#: chatter/parent__allnpcshared:322
msgctxt "chatter/parent__allnpcshared:322:DefaultText"
msgid "Hmm?"
msgstr "Hmm?"

#: chatter/parent__allnpcshared:323
msgctxt "chatter/parent__allnpcshared:323:DefaultText"
msgid "Huh?"
msgstr "Cože?"

#: chatter/parent__allnpcshared:324
msgctxt "chatter/parent__allnpcshared:324:DefaultText"
msgid "Hey."
msgstr "Ahoj."