msgstr ""

#: quests/04_monarch/pq_0401_themaninthehighcastle:30000
msgctxt "quests/04_monarch/pq_0401_themaninthehighcastle:30000:DefaultText"
msgid "You discovered the location of the chemicals and routed the data to Phineas."
msgstr ""

#: quests/04_monarch/pq_0401_themaninthehighcastle:30001
msgctxt "quests/04_monarch/pq_0401_themaninthehighcastle:30001:DefaultText"
